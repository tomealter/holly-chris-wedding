<div class="section" id="travel">
  <div class="section__header">
    <div class="l-constrain">
      <h2>{{ travel_title }}</h2>
    </div>
  </div>
  <div class="travel">
    <div class="l-constrain">
      <div class="flex-grid">
        {% for card in travel_cards %}
          <div class="flex-column">
            {% include "partials/card.html.twig" with {
              title: card.title,
              summary: card.summary,
              eyebrow: card.code,
              url: card.url
            }%}
          </div>
        {% endfor %}
      </div>
    </div>
  </div>
</div>